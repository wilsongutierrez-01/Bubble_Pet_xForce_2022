    <!--HowtoUse-->
    <color name="colorTextContSpaceTitleHowtoUse">@color/gris6</color>
    <color name="colorTextContSpaceInfoHowtoUse">@color/gris11</color>
    <dimen name="paddingVContSpaceHowtoUse">20sp</dimen>
    <dimen name="paddingVContSpaceTitleHowtoUse">15sp</dimen>
